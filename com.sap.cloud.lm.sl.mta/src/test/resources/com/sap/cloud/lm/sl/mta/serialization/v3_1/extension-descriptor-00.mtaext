ID: mta-extension
ext_description: description # this comment should not affect the resulting descriptor
extends: mta-id
ext_provider: provider-id
_schema-version: 3.1.0
target-platforms:
  - first-target
  - second-target
modules:
- name: test-module
  properties:
    module-property: property
  parameters:
    module-parameter: parameter
  provides:
    - name: provided-dependency
      properties:
        provided-property: some-property
  requires:
    - name: required-dependency
      properties:
        required-property: property-value
      parameters:
        required-parameter: parameter-value
resources:
- name: test-resource
  properties:
    resource-property: some-resource-property
  parameters:
    resource-parameter: some-resource-parameter
parameters:
  descriptor-property: some-descriptor-property